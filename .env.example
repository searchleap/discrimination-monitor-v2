# Database
DATABASE_URL="postgresql://user:password@localhost:5432/ai_discrimination"

# AI Services
OPENAI_API_KEY="sk-..."
ANTHROPIC_API_KEY="sk-ant-..."

# External Services
REDIS_URL="redis://localhost:6379"

# Application
NEXTAUTH_SECRET="your-secret-key-here"
NEXTAUTH_URL="http://localhost:3000"

# RSS Processing
RSS_PROCESSING_INTERVAL="0 6 * * *"
RSS_BATCH_SIZE="10"
ENABLE_AI_CLASSIFICATION="true"

# AI Processing Worker Configuration
AI_WORKER_ENABLED="true"
AI_WORKER_BATCH_SIZE="5"
AI_WORKER_INTERVAL="30000"
AI_WORKER_MAX_CONCURRENT="1"
AI_WORKER_AUTO_START="true"
AI_WORKER_HEALTH_CHECK="60000"

# AI Queue Configuration
AI_QUEUE_BATCH_SIZE="5"
AI_QUEUE_INTERVAL="30000"
AI_QUEUE_MAX_RETRIES="3"

# Features
ENABLE_ANALYTICS="true"
ENABLE_EXPORTS="true"
ENABLE_ALERTS="true"

# Automation & Cron Jobs
CRON_SECRET="your-secure-cron-secret-here"
ENABLE_AUTO_PROCESSING="true"
AUTO_PROCESSING_SCHEDULE="*/15 9-17 * * 1-5"